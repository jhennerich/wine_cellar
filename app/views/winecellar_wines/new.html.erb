<h1>Create a new wine for <%= @winecellar.name %></h1>
<p></p>
<%= form_with url: "/winecellars/#{@winecellar.id}/wines", method: :post, local: true do |form| %>
<%= form.label :name %>
<%= form.text_field :name %><p>
<%= form.label :varietal %>
<%= form.text_field :varietal %><p>
<%= form.label :quantity %>
<%= form.number_field :quantity %><p>
<%= form.label :year %>
<%= form.number_field :year %><p>
<%= form.label :winescore %>
<%= form.number_field :wine_score %><p>
<!--
<%= form.label :full %>
<%= check_box_tag "full", "true", "checked" %><p>
  -->

<%= form.submit 'Create Wine' %>
<% end %>
